<% title = 'Timeline' %>

<div class="container mt-5 mb-5">
    <div class="row">
        <div class="col-md-10 offset-md-1">
            <ul class="timeline">
                <% updates.forEach(function(update, index){ %>
                <li>
                    <h6><%= update.date %></h6>
                    <a href="#" class="float-right"></a>
                    <p><%= update.description %></p>
                </li>
                <% }); %>
            </ul>
        </div>
    </div>
</div>

